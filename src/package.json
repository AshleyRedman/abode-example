{
  "name": "src",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "create:block": "plop",
    "tw:dev": "tailwindcss -i ./input.css -o ./dist/output.css",
    "tw:watch": "tailwindcss -i ./input.css -o ./dist/output.css --watch",
    "tw:prod": "tailwindcss -i ./input.css -o ./dist/output.css --minify",
    "mix:dev": "npx mix",
    "mix:watch": "npx mix watch",
    "mix:prod": "npx mix --production",
    "dev": "concurrently \"npm run tw:dev\" \"npm run mix:dev\"",
    "watch": "concurrently \"npm run tw:watch\" \"npm run mix:watch\"",
    "prod": "npm run mix:prod && npm run tw:prod"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@babel/preset-react": "^7.18.6",
    "@types/react": "^18.0.15",
    "concurrently": "^7.3.0",
    "laravel-mix": "^6.0.49",
    "laravel-mix-bundle-analyzer": "^1.0.5",
    "plop": "^3.1.1",
    "tailwindcss": "^3.1.6",
    "ts-loader": "*",
    "typescript": "^4.7.4"
  },
  "dependencies": {
    "react": "^18.2.0",
    "react-abode": "^1.1.0",
    "react-dom": "^18.2.0"
  }
}
